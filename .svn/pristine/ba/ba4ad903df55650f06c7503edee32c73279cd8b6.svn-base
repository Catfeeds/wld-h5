.gcategory-main{padding-top: 2%;}
.gcategory-box{ background: #fff; width: 94%;margin:2.5% auto 0 auto;border-radius: 8px;padding: 0 3%;}
.g-box-top{padding: 4% 0;}
.g-box-tfl{width: 90%;}
.g-box-tfr{width: 8%;}
.g-box-bottom{border-top:solid 1px #DFDFDF;padding: 4% 0;}
.g-box-bottom .icon-box{float: left;width:22%;margin-left: 3%;}
.gbox-icon{width: 26%;margin:0 5%;}
.gbox-font{width: 64%;}

.add-gcategory{padding: 2.8%;text-align: center;}

.add-gcate-bg{}
.add-gcate-con{width:75%;position:fixed;top:32%;left:0;right:0;margin:auto;background:#fff;border-radius: 20px;z-index: 999;display: none;}
.gcate-tt{text-align: center;padding: 5% 4%;}
.gcate-name{padding: 5% 4% 10% 4%;}
.gcate-name input{background: #EFF0F5;text-align: center;padding: 4% 2%;width: 100%;border:none;outline: none;}
.gcate-name span{width: 100%;text-align: center;display: block;color: red; visibility: hidden;}
.gcvate-btn{border-top:solid 1px #DFDFDF;text-align: center;}
.gcvate-btn span{display: block;width: 50%;float: left;padding: 4% 0;}
.gcvate-btn span:nth-of-type(1){border-bottom-left-radius:20px;border-right:solid 1px #DFDFDF;}
.gcvate-btn span:nth-of-type(2){border-bottom-right-radius:20px;}