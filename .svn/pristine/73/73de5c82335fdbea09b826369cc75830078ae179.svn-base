<!DOCTYPE HTML>
<html>
<head>
<meta charset="utf-8">
<meta name="renderer" content="webkit|ie-comp|ie-stand">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,member-scalable=no" />
<meta http-equiv="Cache-Control" content="no-siteapp" />
<script type="text/javascript">var SKIN = "__HSKIN__";var HHOME = "__HHOME__";</script>
<!--[if lt IE 9]>
<script type="text/javascript" src="__HLIB__/html5.js"></script>
<script type="text/javascript" src="__HLIB__/respond.min.js"></script>
<script type="text/javascript" src="__HLIB__/PIE_IE678.js"></script>
<![endif]-->
<link href="__HCSS__/H-ui.min.css" rel="stylesheet" type="text/css" />
<link href="__HCSS__/H-ui.admin.css" rel="stylesheet" type="text/css" />
<link href="__HLIB__/icheck/icheck.css" rel="stylesheet" type="text/css" />
<link href="__HLIB__/Hui-iconfont/1.0.1/iconfont.css" rel="stylesheet" type="text/css" />
<!--[if IE 6]>
<script type="text/javascript" src="http://lib.h-ui.net/DD_belatedPNG_0.0.8a-min.js" ></script>
<script>DD_belatedPNG.fix('*');</script>
<![endif]-->
<title>分润明细</title>
</head>
<body>
<div class="pd-20">
	<form action="" method="post" class="form form-horizontal" id="form-category-add">
		<div id="tab-category" class="HuiTab">
				<div class="tabCon">	
				    <div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>选择分润人：</label>
						<div class="formControls col-6"> <span class="select-box">
							<select name="ucode" class="select" onClick="selectucode('请选择分润人','Member/member_list?hide=1',800);" id="typecheck">
								<option value="{$vo.c_ucode}">{$vo.c_nickname}</option>
							</select>
							</span> 
						</div>
					</div>
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>订单来源：</label>
						<div class="formControls col-6">
							<span class="select-box">
						        <select class="select" id="sel_Sub" name="c_source">
							        <option value=""> </option>
							        <option value="1" <if condition="$vo.c_source eq 1">selected="selected"</if>>普城订单</option>
							        <option value="2" <if condition="$vo.c_source eq 2">selected="selected"</if>>后台</option>
							        <option value="3" <if condition="$vo.c_source eq 3">selected="selected"</if>>活动</option>
							        <option value="4" <if condition="$vo.c_source eq 4">selected="selected"</if>>蜜城订单</option>
							        <option value="5" <if condition="$vo.c_source eq 5">selected="selected"</if>>普通跨界</option>
							        <option value="6" <if condition="$vo.c_source eq 6">selected="selected"</if>>提现</option>
							        <option value="7" <if condition="$vo.c_source eq 7">selected="selected"</if>>注册</option>
							        <option value="8" <if condition="$vo.c_source eq 8">selected="selected"</if>>老注册</option>
							        <option value="9" <if condition="$vo.c_source eq 9">selected="selected"</if>>扫码</option>
							        <option value="10" <if condition="$vo.c_source eq 10">selected="selected"</if>>转发</option>
							        <option value="11" <if condition="$vo.c_source eq 11">selected="selected"</if>>绑定</option>
							        <option value="12" <if condition="$vo.c_source eq 12">selected="selected"</if>>跨界扫码</option>
							        <option value="13" <if condition="$vo.c_source eq 13">selected="selected"</if>>普城购返</option>
							        <option value="14" <if condition="$vo.c_source eq 14">selected="selected"</if>>普城推返</option>
							        <option value="15" <if condition="$vo.c_source eq 15">selected="selected"</if>>蜜城跨界</option>
							        <option value="16" <if condition="$vo.c_source eq 16">selected="selected"</if>>普通退款</option>
							        <option value="17" <if condition="$vo.c_source eq 17">selected="selected"</if>>蜜城退款</option>
							        <option value="18" <if condition="$vo.c_source eq 18">selected="selected"</if>>红包</option>
							        <option value="20" <if condition="$vo.c_source eq 20">selected="selected"</if>>充值</option>
							        <option value="21" <if condition="$vo.c_source eq 21">selected="selected"</if>>手续费</option>
						        </select>
						    </span>
						</div>
					</div>
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>来源描述：</label>
						<div class="formControls col-6">
							<input type="text" class="input-text"  value="{$vo.c_desc}" placeholder="" id="" name="c_desc" nullmsg="来源描述不能为空">
						</div>
					</div>
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>来源关键字：</label>
						<div class="formControls col-6">
							<input type="text" class="input-text"  value="{$vo.c_key}" placeholder="" id="" name="c_key" nullmsg="来源关键字不能为空">
						</div>
					</div>	
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>分到的金额：</label>
						<div class="formControls col-6">
							<input type="text" class="input-text"  value="{$vo.c_money}" placeholder="" id="" name="c_money" nullmsg="分到的金额不能为空">
						</div>
					</div>
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>结算方式：</label>
						<div class="formControls col-6">
							<span class="select-box">
					          	<select class="select" id="sel_Sub" name="c_type">
					          	    <option value=""> </option>
						            <option value="1" <if condition="$vo.c_type eq 1">selected="selected"</if>>实时结算</option>
						            <option value="2" <if condition="$vo.c_type eq 2">selected="selected"</if>>按日结算</option>
						            <option value="3" <if condition="$vo.c_type eq 3">selected="selected"</if>>按月结算</option>
					            </select>
					        </span>
						</div>
					</div>	
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>状态：</label>
						<div class="formControls col-6">
						    <span class="select-box">
					          	<select class="select" id="sel_Sub" name="c_sign">
					          	    <option value=""> </option>
									<option value="1" <if condition="$post['c_sign']  eq 1">selected="selected"</if>>代付</option>
		     		                <option value="2" <if condition="$post['c_sign']  eq 2">selected="selected"</if>>代扣</option>
		     		            </select>
					        </span>
						</div>
					</div>
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>是否结算</label>
						<div class="formControls col-6">
						    <span class="select-box">
					          	<select class="select" id="sel_Sub" name="c_settled">
					          	    <option value=""> </option>
						     		<option value="1" <if condition="$post['c_settled']  eq 1">selected="selected"</if>>已结算</option>
						     		<option value="2" <if condition="$post['c_settled']  eq 2">selected="selected"</if>>未结算</option>
		     		            </select>
					        </span>
						</div>
					</div>
					<div class="row cl">
						<label class="form-label col-3"><span class="c-red">*</span>是否完成：</label>
						<div class="formControls col-6">
						    <span class="select-box">
					          	<select class="select" id="sel_Sub" name="c_status">
					          	    <option value=""> </option>
									<option value="dj" <if condition="$post['c_status']  eq 'dj'">selected="selected"</if>>未完成</option>
						     		<option value="1" <if condition="$post['c_status']  eq 1">selected="selected"</if>>已完成</option>
						     		<option value="2" <if condition="$post['c_status']  eq 2">selected="selected"</if>>已取消</option>
		     		            </select>
					        </span>
						</div>
					</div>
				</div>
			<div class="row cl">
				<div class="col-9 col-offset-3">
					<input type="hidden" name="Id" value="{$vo.c_id}">
					<div class="btn btn-primary radius" onclick="ajax_post_data('{$action}','form-category-add','Member/share_list');">&nbsp;&nbsp;提交&nbsp;&nbsp;</div>
				</div>
			</div>
		</div>
	</form>
</div>
<script type="text/javascript" src="__HLIB__/jquery/1.9.1/jquery.min.js"></script> 
<script type="text/javascript" src="__HLIB__/icheck/jquery.icheck.min.js"></script> 
<script type="text/javascript" src="__HLIB__/Validform/5.3.2/Validform.min.js"></script> 
<script type="text/javascript" src="__HLIB__/layer/1.9.3/layer.js"></script> 
<script type="text/javascript" src="__HJS__/H-ui.js"></script> 
<script type="text/javascript" src="__HJS__/H-ui.admin.js"></script> 
<script type="text/javascript">
$(function(){
	$('.skin-minimal input').iCheck({
		checkboxClass: 'icheckbox-blue',
		radioClass: 'iradio-blue',
		increaseArea: '20%'
	});
	
	$("#form-article-category-add").Validform({
		tiptype:2,
		callback:function(form){
			form[0].submit();
			var index = parent.layer.getFrameIndex(window.name);
			parent.$('.btn-refresh').click();
			parent.layer.close(index);
		}
	});
	$.Huitab("#tab-category .tabBar span","#tab-category .tabCon","current","click","0");
});
// 选择分润人
function selectucode(title,url,w,h) {
	layer_show(title,url,w,h);
	$('.ctrl_hidden').hide();
}
</script>
</body>
</html>