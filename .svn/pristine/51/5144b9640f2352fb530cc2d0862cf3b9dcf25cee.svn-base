@charset "utf-8";
/* CSS Document */
img{width: 100%;height: 100%;}


/* 创建秒杀 */
.c-create-con{margin-bottom: 3%;}
.c-create-con li{border-bottom: solid 1px #ebebeb;padding: 3.5% 0;overflow: hidden;}
.c-creatright{width: 6%;margin-right: 2%;}
.c-creatleft{width: 25%;text-align: center;}
.c-creatcenter{width: 64%;margin-left: 3%;}
.c-creatcenter input{}
.c-selected-pro .c-pro-list{margin-left: 3%;overflow: hidden;width: 97%;padding: 3% 3% 0 0;}
.c-pro-li .c-pro-list{margin-left: 3%;overflow: hidden;width: 97%;padding: 3% 3% 3% 0;}
.c-pro-lied{background: #f0f0f0;}
.c-pro-img{border:solid 1px #f0f0f0;width:25%;}
.c-pro-info{width: 72%;margin-left: 3%;}


.c-model-list{ display: none; background: #fff; position: fixed;border-top: 1px solid #e5e5e5;left: 0;right: 0; bottom: 0; margin: 0 auto;width: 100%;max-width: 720px;min-width: 320px;z-index: 1000;}
.c-model-list ul{overflow: hidden;width: 100%;padding: 2% 0 5% 0;}
.c-model-list ul li{border-radius: 5px;text-align: center;margin:3% 0 0 3%;padding: 2% 0;background: #F0F0F0;color: #999;float: left;width: 29.3%;}
.c-model-list ul li.hover{background: #47ABF7;color: #fff;}
.c-mode-sure{width: 45%;margin: 5% auto; text-align: center;border-radius: 5px;background: #47ABF7;padding: 2% 0;}


/* 限时秒杀首页 */
.comm-nav-tab{padding: 0 0 0 0;-webkit-box-shadow:0 0 6px rgba(0, 0, 0, .1);-moz-box-shadow:0 0 6px rgba(0, 0, 0, .1);box-shadow:0 0 6px rgba(0, 0, 0, 0.1);margin-bottom: 1%;}
.comm-nav-tab ul{overflow: hidden;}
.comm-nav-tab ul li{float: left;padding: 3% 0;width: 33.3%;text-align: center;color: #999;}
.comm-nav-tab ul li.hover{border-bottom: solid 2px #47ABF7;color: #47ABF7;}

.sjcollage-con{ padding-bottom: 18%;}
.sjcollage-li{margin-bottom: 3%;}
.sjcollage-pro{padding: 3%;}
.sjc-pro-img{width: 30%;}
.sjc-pro-info{width: 67%;margin-left: 3%;}
.sjc-pro-price span{ text-decoration: line-through; padding-left: 8%;}
.sjcollage-oprate ul li{float: left;text-align: center;padding: 3% 0;width: 33.3%;}
.oprate_3 ul li{width: 50%;}
.sjc-state{width: 15%;right: 2%;top: 45%;}
.ad-creat-site{background: #fff; position: fixed;border-top: 1px solid #e5e5e5;left: 0;right: 0; bottom: 0; margin: 0 auto;width: 100%;max-width: 720px;min-width: 320px;padding: 3.5% 0;text-align: center;z-index: 99;}
.ad-creat-btn{width: 55%;margin: 0 auto;}


/*秒杀详情*/
.collage-box{margin-bottom: 4%;}
.c-box-info{ padding: 3%;}
.c-box-name{ margin-bottom: 5%;}
.c-box-price { width: 75%; padding-top: 2.5%;}
.c-box-price span{ margin-left: 8%; text-decoration: line-through;}
.c-box-number{ width: 38%; background-size: 100% 100%;}
.c-box-number span:nth-of-type(1){ left:10%; top: 15%;}
.c-box-number span:nth-of-type(2){ left: 56%; top: 12%;}


/* 团购详情页css */
.cgood-open-status{ bottom: 0;z-index: 999;}
.cgood-open-con{ padding: 0 3%;}
.cgood-open-con{ bottom: 10%;padding: 0 3%;}
.cgood-number{ top: 10%; text-align: center;}
.cgood-time{padding-top: 3%;}
.good-name{padding: 3% 3% 0 3%;}
.good-price{padding: 3%;}
.agent-intro {margin: 2.5% auto;padding: 3%;}
.ag-head {width: 14%;overflow: hidden;}
.ag-info {width: 60%; margin-left: 3%;}
.ag-visitnum span { margin: 0 2%;}
.ag-attention{ width: 23%;}
.ag-comment{ margin-top: 2.5%}
.good-remark {padding: 3%; margin-top: 2.5%;}
.view-collage-number{margin-top: 2.5%;text-align: center;padding: 4% 0;}
.view-all{ margin-right: 5%; text-align: right;}
.view-all .font{ margin-left: 3%;}


.ag-com-tit{margin-left: 3%;padding: 3% 4% 3% 0;}
.ag-comnumb{width: 70%;}
.ag-comcheck{width: 30%;text-align: right;}
.ag-comcheck span{display: block;width: 22.5%;}
.ag-nkname span{ background: #ffaa3b;padding: 0 2%;margin-left: 3%;}
.comment-con{padding: 3%;}
.comm-head{width: 14%;overflow: hidden;}
.comm-intro{width: 83%;margin-left: 3%;overflow: hidden;}
.comm-nc-grade{overflow: hidden;}
.comm-nc-grade p{overflow: hidden;width: 100%;}
.comm-nc-grade p span{display: block;width: 25%;float: left;}
.comm-desc{padding: 1% 0 3% 0;}
.comm-imglist ul{overflow: hidden;}
.comm-imglist ul li{width: 32%;margin: 2% 1.3% 0 0; overflow: hidden;float: left;}

.good-remark{padding: 3%;margin-top: 2.5%;}
.good-remark p{padding: 2% 0;line-height: 1.75em;}
.good-d-imglist{max-width: 720px;}


.good-b-btn{padding: 0 0 0 3%;}
.g-carticon{width: 12%;padding: 2% 0;}
.g-cartnum{top: 0;right: 0;background: #ffaa3b;border-radius: 100%; border: solid 1px #fff; padding: 0 3%;}
.g-total-price{width:  55%;padding: 2% 0;text-align: left;margin-left: 3%;}
.g-add-cart{width: 30%;background: #FDAB3D;padding: 2% 0;}

/*选择型号*/
.mode-sel-bg{}
.model-sel-pup{display: none; position: fixed;left: 0;right: 0;width: 100%;margin: 0 auto;bottom: 6%;z-index: 998;max-width: 720px;padding-bottom: 6%;}
.model-sel-main{}
.m-pro-basic{padding:3% 3% 3% 0;margin-left: 3%;overflow: hidden;}
.m-pro-img{width: 18%;overflow: hidden;}
.m-pro-info{width: 71%;margin-left: 3%;}
.m-pro-info p span{margin-right: 3%;}
.m-sel-close{width: 8%;right: 3%;top: 3%;}

.m-sel-list{padding: 3% 3% 3%　0;margin-left: 3%;width: 97%;}
.m-sel-list-t{padding: 3% 0 0 0;}
.m-seleted-c{padding: 2% 3% 3% 0;overflow-y: auto;-ms-overflow-y: auto;margin: 1% 0 3% 0;}
.m-seleted-c ul{overflow: hidden;width: 100%;}
.m-seleted-c ul li{background: #f0f0f0;padding: 1% 2.5%;border-radius: 5px;color: #999;float:left;margin: 0 2% 2% 0;width: 23%;text-align: center;}
.m-seleted-c ul li.hover{background: #47ABF7;color: #fff;}

.m-seleted-c span{background: #f0f0f0;padding: 1% 2.5%;border-radius: 10px;color: #999;margin: 2% 2% 2% 0;}
.m-seleted-c span.hover{background: #47ABF7;color: #fff;}

.m-pro-number{padding: 3% 3% 3%　0;margin-left: 3%;overflow: hidden;}
.m-num-opreate{padding: 3% 0;}
.m-num-sub{width: 10%;padding: 0;border-radius: 5px;}
.m-num-txt{width: 20%;overflow: hidden;margin: 0 2%;}
.m-num-txt input[type="text"]{ background: #f0f0f0; padding: 12% 0;border-radius: 5px;width: 100%;}
.m-num-add{width: 10%;padding: 0;border-radius: 5px;}

.pro-price-rebate{overflow: hidden;width: 100%;padding: 0 0 3% 3%;}
.shop-rebate,.share-rebate{color: #555;float: left;overflow: hidden;margin-right: 3%;}
.shop-rebate span,.share-rebate span{color: #fbac4d;}
.product-stock{width: 40%;color: #555;margin-left: 3%;}
.product-stock span{color: #333;}

.m-btn-list{ position: fixed; bottom: 0;left: 0;right: 0;margin: 0 auto;max-width: 720px;}
.m-btn-list span{display: block; background: #45AAFA; width: 100%;text-align: center;padding: 2.5% 0;}


/*加入购物车，立即购买案按钮*/
.pro-buy-btn{overflow:hidden;position: fixed;border-top:solid 1px #ebebeb;width: 100%;max-width: 720px;min-width: 320px; margin:auto;left:0;right:0;bottom:0;z-index: 999;}
.add-shopping-cart,.shopping-cart,.add-groupbuy{display: block; cursor:pointer; color: #fff;text-align: center;}
.add-shopping-cart{background: #FDAB3D;width: 35%;padding: 4% 0 0 0;}
.shopping-cart{background: #FF6F33;width: 35%;}
.add-groupbuy{background: #FF6F33;width: 35%;padding: 4% 0 0 0;}
.server-btn,.space-btn{border-right: solid 1px #d8d8d8;}
.server-btn,.space-btn{width: 15%;display: block;background: #fff;}

/*记录*/
.gbuy-data-list ul{overflow: hidden;}
.gbuy-data-list ul li{margin-left: 3%;border-bottom: solid 1px #ebebeb;padding: 3% 3% 3% 0;width: 97%;overflow: hidden;}
.collage-pro-data { padding-left: 3%;}
.collage-pro-data div{border-bottom: solid 1px #ebebeb;}
.collage-data{ float: left;width:50%;text-align: center;padding: 4% 0;}
.collage-data p:nth-of-type(2){ padding-top: 3%;}
.u-headinfo{width: 18%;overflow: hidden;}
.u-headimg{width: 100;left: 0;top: 0;bottom: 0;right: 0;overflow: hidden;}
.u-nameinfo{width:54%;margin-left: 3%;padding-top: 2%;}
.u-state{width: 25%;text-align: center;padding-top: 2%;}
.u-state-img span{ margin-right: 15%;}


/*帮助*/
.help-con{padding: 5% 3% 5% 3%;}
.help-title{ margin-bottom: 4%;}
.help-introduce{ margin-bottom: 8%;}
.help-phone{ text-align: center;}
.help-phone{margin-top: 20%;}
.h-phone{margin-top: 1%;}
.help-introduce span { display: block; margin-bottom: 3%;}