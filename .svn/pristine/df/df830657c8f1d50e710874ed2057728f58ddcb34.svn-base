
/*激活码管理*/
.activation{width: 96%;margin:3%;background:#fbf8f8;padding:3%;}
.activation_top{width: 80%;overflow: hidden;border-bottom: 1px solid #ebebeb;}
.activation_title{width: 35%;padding:1% 0px;border-bottom: 1px solid #58c3f0;
overflow: hidden;text-align: center;font-size: 18px;font-weight: 400;color:#666;}
.activation_content{width: 60%;overflow: hidden;}
.activation_list{padding:2% 1%;width: 46%;overflow: hidden;position:relative;}
.activation_list img,.activation_abimg img{width: 100%;height: auto;border: 1px solid #ebebeb;}
.activation_abimg{width: 96%;overflow: hidden;position:absolute;z-index: 222;
top:5%;left:2%;}
.activation_text{width: 96%;overflow: hidden;position:absolute;z-index: 22;color:#fff;
font-size: 22px;top:40%;left:2%;text-align: center;font-weight: 600;}
.activation_nobel{width: 92%;overflow: hidden;position:absolute;z-index: 22;color:#fff;
font-size: 14px;top:75%;left:2%;text-align: right;font-weight: 600;}
.activation_form{width: 72%;position:absolute;z-index: 22;color:#333;overflow: hidden;
font-size: 14px;top:25%;left:13%;display:none;}
.activation_redio{width: 100%;overflow: hidden;padding:2% 4%;}
.activation_but{width: 40%;margin-left: 30%;text-align: center;border-radius: 5px;color:#fff;
font-size: 14px;background:#58c3f0;margin-top: 5%;padding: 2% 0px;cursor:pointer;}
.produce_checks{color: #47abf7;}


/*企业代理申请样式*/
.business{width: 90%;margin:2%;background:#fff;padding:3%;}
.business_top{width: 80%;overflow: hidden;border-bottom: 1px solid #ebebeb;}
.business_title{width: 150px;padding:1% 0px;border-bottom: 1px solid #58c3f0;
overflow: hidden;text-align: center;font-size: 18px;font-weight: 400;color:#666;}
.business_mid{width: 60%;overflow: hidden;padding:2% 0px;}
.business_mid img{width: 100%;height: auto;}
.business_sign{width: 120px;padding:1% 0px;overflow: hidden;font-size: 16px;color:#666;
color:#666;}
.business_content{width: 80%;overflow: hidden;}
.inpu_info{width: 74.5%;overflow: hidden;font-size: 14px;color:#666;border:1px solid #ebebeb;
margin: 1% 0px;}
.inpu_name{width: 20%;overflow: hidden;padding:1.5%;}
.inpu_text{width: 74%;overflow: hidden;padding:1.5%;border:0px;font-size: 14px;font-size: 14px;}
.inpu_length{width: 36%;}
.inpu_lgname{width: 35%;padding:2%;}
.inpu_lgtxet{width: 55%;padding:2%;}
.business_content label{font-size: 12px;color:#58c3f0;padding:2%;}
.business_img{width: 20%;overflow: hidden;margin: 3%;}
.business_img img{width: 100%;height:auto;}
.business_sub{width: 15%;padding:1% 0px;border-radius: 3px;color:#fff;
font-size: 16px;background:#58c3f0;border:0px;margin: 3%;}
.business_tit{width: 80%;height: 25px;line-height: 25px;border-left:3px solid #58c3f0;overflow: hidden;
padding-left: 15px;font-size: 16px;color:#555;}
.business_desc{width: 80%;padding:1% 0px;margin-left: 2%;font-size: 12px;color:#58c3f0;}
.business_coun{width: 60%;overflow: hidden;padding:2%;font-size: 18px;color:#555;
font-weight: 400;}




