<script type="text/javascript">
function order_action(ucode){
	$.post('__URL__/synchronize',{ucode:ucode},function(data){
		var result = eval(data);		
		if(result['code'] != 0){
			layer.msg(result['msg'],{icon:10,time:2000});
		}else{
			layer.msg(result['msg'],{icon:1,time:2000});
			setTimeout(function() {
				window.location.reload();
			}, 1000);			
		}
	});
}

//商户进件
function PostAddmerchant(ucode) {
	layer.confirm('进件前请先仔细审核商户资料！',function(index){
		tjpost(ucode);
	});
}

var kongzhi = true;
function tjpost(ucode) {
	if (kongzhi) {
		kongzhi = false;
		//此处请求后台程序，下方是成功后的前台处理……	
		$.post(HHOME+'/Agent/PostAddmerchant',{ucode:ucode}, function(obj) {
			var result = eval(obj);					
	        if (result['code'] != 0) {
	        	layer.msg(result['msg'],{icon:10,time:2000});  
	        	kongzhi = true;	      	      	
	        } else {
	        	layer.msg(result['msg'],{icon:1,time:2000});
	        	window.location.reload(); 
	        }        
	    });		
	}
}
</script>
