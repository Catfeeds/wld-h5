	/*获取菜单列表*/
	function GetServerMenu(){
		var _html="";
		$.ajax({
			type: 'get',
			dataType: 'json',
			url: '__URL__/GetServerMenu',
			cache: false,
			beforeSend: function() {
				$('#console').css('display', 'block');
				$('#console').html('加载中...');
				ctrls = false;
			},
			error: function() {
				$('#console').css('display', 'block');
				$('#console').html('加载失败');
				ctrls = true;
			},
			success: function(obj) {
				console.log(obj)
				var msg = eval(obj);
				if(msg['code'] == 0) {
					var data = msg.data;
					if(data == null) {
						_html += '<div class="baoqian">没有找到相关信息</div>';
						emptyval = false;
					} else {
						for(var i = 0;i<data.length;i++){
							_html+='<div class="server-menu-list bgcolor divtab">';
							if(data[i]['name']){						
								_html+='<div class="server-menu-tit fs15 c3">'+data[i]['name']+'</div>';
							}else{
								_html+='<div class="server-menu-tit fs15 c3 none"></div>';
							}
							_html+='<div class="server-menu-con divtab">';
							var datalist = data[i]['menu'];
							for(var k = 0; k<datalist.length;k++){
								_html+='<div class="menu-con-box" onclick="tiaozhuan(\''+datalist[k]['c_interface_type']+'\',\''+datalist[k]['c_interface_address']+'\',\''+datalist[k]['c_alias']+'\')">';
								_html+='	<div class="s-menu-ico"><img src="'+datalist[k]["c_img"]+'" alt="" /></div>';
								_html+='	<div class="s-menu-txt c5 fs12">'+datalist[k]["c_name"]+'</div>';
								_html+='</div>';						
							}
							_html+='</div>';
							_html+='</div>';
						}						
					}
				} else {
					emptyval = false;
				}
				$('.server-menu-main').append(_html);
			},
			complete: function() {
				styles();
				$('#console').css('display', 'none');
				ctrls = true;
			}
		});	
	}
	/*菜单跳转*/
	function tiaozhuan(type,url,alias){
		if (type == 1) {
			mui.openWindow({url:url,id:alias});
		} else {
			if(!ucode){
	 			mui.openWindow({url:'__APP__/Login/Index/index',id:'login'}); 			
	 		}else{
	 			mui.openWindow({url:url,id:alias});
	 		}
		}
	}
	
