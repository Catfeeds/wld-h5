

.impower-main{position: relative;width: 100%;max-width: 720px;min-width: 320px;}
.impower-logo{position: absolute;right: 13%;top: 13%;width: 45%;text-align: center;}
.impower-input{background: #fff;border-radius: 12px;width: 84%;margin: 0 auto;left: 0;right: 0;top: 25%;position: absolute;}
.impower-phone{border-bottom: solid 1px #bfd7f9;width: 100%;overflow: hidden;padding: 3% 0;}
.impower-phone input[type="text"]{ border: none;background: #fff; color: #999;padding: 3% 2%;float: left;width: 82%;outline: none;}
.impower-phone span{color: #999;float: right;display: block;width: 15%;padding: 3% 0;margin-right: 3%;text-align: center;}
.impower-incode{border: none;}
.impower-sub{ border-radius: 12px; background: #fff;color: #47abf7;text-align: center;padding: 2% 0;width: 84%;margin: 5% auto;position: absolute;top: 67%;left: 0;right: 0;}
.impower-sub:hover{background:#1770FF;color: #fff; }
.impower-font{color: #fff;position: absolute;top: 46%;left:8%;right: 8%;margin: 0 auto;}
.impower-font p{padding: 1% 0;}


#regress{background: #def5fb;}
.regress-top{width: 100%;overflow: hidden;max-width: 720px;min-width: 320px;}
.regress-register{width: 86%;margin: 0 7%;background: #5cbbfb;border-radius: 10px;padding-bottom: 6%;}
.regress-reg-tit{margin: 0 auto;width: 28%;text-align: center;}
.regress-reg-tit img{width: 100%;height: 100%;margin-top: -6.5%;}
.regress-reg-con{margin: 0 auto;width: 100%;}
.regress-reg-box{border-bottom: solid 1px #fff;width: 100%;overflow: hidden;}

.regress-txt{ border:none; color: #fff;padding: 5% 2% 5% 5%;width: 72%;outline: none;}
.regress-reg-box :-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #fff;  
}
.regress-reg-box ::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #fff;
}
.regress-reg-box input:-ms-input-placeholder{
    color: #fff;
}
.regress-reg-box input::-webkit-input-placeholder{
    color: #fff;
}
.regress-duanx{ border: none; background: url(../../img/impower/regress-8.png) no-repeat;background-size: 100% 100%; cursor:pointer; width: 25%;text-align: center;color: #5cbbfb;border-radius: 8px;padding: 2% 0;margin-top: 4.5%;margin-right: 3%;outline: none;}
.reg-txt-6{overflow: hidden;width: 92%;margin:5% auto;}
.agree_left{float: left;}
.agree_right{color: #fff;float: left;width: 85%;margin-left: 3%;}
.agree_right a{color: #fff;}
#registerbtn{width: 85%;margin: 0 auto;cursor:pointer;}

#regress-two{margin: 10% auto;}
#rule-regress{padding-top: 4%;}
#rule-regress p{color: #fff;padding: 1% 3%;}

/*推荐注册*/
.agree_new_left{padding: 5% 0% 5% 5%;}
.agree_new_right{padding: 5% 0% 5% 0%;width: 83%;}
.reg_box{border-bottom: 0px;}
.agree_size{}
.invite{color:#114d9d;padding:5% 2% 8% 5%;width: 75%;}
.invite_reason{width: 86%;margin:0 7% 5% 7%;padding:0 2% 3% 2%;color:#555;line-height:1.5;}
.invite-top{position: relative;z-index: 99;}
.invite-headimg{position:absolute;top:66%;left:26%;width: 11%;border:1px solid #fff;overflow: hidden;}
.invite-headimg img{width: 100%;height: 100%;}
.invite-name{position:absolute;top:64%;left:40%;width: 55%;overflow: hidden;color:#fff;padding:1% 0px;}



input[type="checkbox"]{ cursor:pointer; width: 20px;height: 20px;border: 0px;-webkit-appearance: none;-webkit-mask-repeat: none;-webkit-mask-position: right;background: url(../../img/impower/regress-10.png);background-size: 100% 100%; outline:none;}
input[type="checkbox"]:checked {width: 20px;height: 20px;border: 0px;background:url(../../img/impower/retress-5.png);background-size: 100% 100%;}
.num_sub{color: #555555;}
@media (min-width: 421px){
    input[type="checkbox"]{width: 25px;height: 25px;background-size: 100% 100%;}
    input[type="checkbox"]:checked {width: 25px;height: 25px;background-size: 100% 100%;}  
}