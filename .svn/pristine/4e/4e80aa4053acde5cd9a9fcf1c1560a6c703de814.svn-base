/*代理包添加*/
.addli{padding: 3% 3%;border-bottom: 1px solid #ebebeb;}
.addli1{border:0px;padding-bottom: 0px;}
.addleft{width: 25%;padding: 2% 0px;}
.addright{width: 70%;padding: 2% 2%;}
.addsub{background: #46aafa;margin: 6% 4%;width: 92%;padding: 3% 0px;text-align: center;border-radius: 5px;}
.upload-img{width: 100%;overflow: hidden;padding:3%;background:#fff;padding-top: 0;border-bottom: 1px solid #ebebeb;}
.upload-title{width: 100%;overflow: hidden;padding:2% 0px;}
.upload-text{width: 100%;padding:3%;overflow: hidden;}
.productmodular-img{position: relative;float:left;width: 30%;margin-right: 3%;margin-top:3%;}
.productmodular-position{position: absolute;width: 21%;top:-5%;right:-5%;overflow: hidden;z-index:11;}
.productmodular-img input[type="file"]{ position: absolute;left: 0;top: 0;bottom: 0; right: 0; z-index: 10;opacity: 0; filter:alpha(opacity=0);}
.productmodular-img img,.productmodular-position img,.productmodular-position img{width: 100%;height: 100%;}
.offonimg{width: 13%;padding-top: 2%;}


