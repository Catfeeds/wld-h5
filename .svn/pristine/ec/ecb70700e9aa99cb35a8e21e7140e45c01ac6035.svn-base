		<include file="Base@Public/pubjs" />
		<script type="text/javascript">
			window.onload = function() {

				styles();
			}

			function styles() {
				var thhh = $('.s-tmp-h-img').width();
				$('.s-tmp-h-img').height(thhh);

				var phlih = $('.s-tmp-photo-li li').width();
				$('.s-tmp-photo-li li').height(phlih);
				
				var dth = $('.s-tmp-p1-dt').height();
				$('.s-tmp-p1-dt').height(dth);
				$('.s-tmp-p1-pro').height(dth);
				
				var prohj = $('.s-tmp-p1-prori').height(),prolihj = prohj*50/100;
				$('.s-tmp-p1-prole').css('line-height',prolihj+'px');
				
				var couh = $('.s-tmp-p1-couimg').height(),couhj = couh*50/100;
				$('.s-tmp-p1-coutt').css('line-height',couhj+'px');
			}

			/*实体商家相册*/
			mui('.mui-content').on('tap', '.s-photo-tmp-li', function() {
				mui.openWindow({
					url: '__URL__/photo',
					id: 'photo'
				});
			});
			mui('.mui-content').on('tap', '.s-photo-more', function() {
				mui.openWindow({
					url: '__URL__/photo',
					id: 'photo'
				});
			});
		</script>
