			/*数据加载*/
			function getdatalist() {
				$('#data_list').empty();
				var name = $('#bankkey').val();
				var url = "__URL__/GetBankList?name=" + name;
				var _html = "";
				$.ajax({
					type: 'get',
					dataType: 'json',
					url: url,
					cache: false,
					beforeSend: function() {
						$('#console').css('display', 'block');
						$('#console').html('加载中...');
					},
					error: function() {
						$('#console').css('display', 'block');
						$('#console').html('加载失败');
					},
					success: function(obj) {
						var mgs = eval(obj);
						if(mgs['code'] == 0) {
							var data = mgs.data;
							if(data){
								for(var i = 0; i < data.length; i++) {
									var dataarr = data[i];
									_html += '<li onclick="sel_fee_bank(\''+dataarr['c_name']+'\')">';
									_html += '<div class="bankcell">';
									_html += ''+dataarr['c_id']+'';
									_html += '</div>';
									_html += '<div class="bankcell">';
									_html += ''+dataarr['c_name']+'';
									_html += '</div>';
									_html += '</li>';
								};								
							}else {
								_html += '<div class="baoqian">没有找到相关信息</div>';	
							}
						} else {
							_html += '<div class="baoqian">没有相关信息</div>';
						}
						$('#data_list').append(_html);
					},
					complete: function() {
						$('#console').css('display', 'none');
					}
				});
			}

