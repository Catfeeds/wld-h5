				if($('#contactline').val() == "") {
					mui.toast('网点号（联行号）不能为空');
					$('#contactline').focus();
					return false;
				}
				if($("#idcardtype").find("option:selected").val() == '请选择') {
					mui.toast('请选择证件类型');
					$('#idcardtype').focus();
					return false;
				}
				if($('#idcardinfo').val() == '') {
					mui.toast('请输入证件号');
					$('#idcardinfo').focus();
					return false;
				}
				var match = /^1[3|4|5|7|8][0-9]\d{8}$/;
				if(!match.exec($('input[name="banktel"]').val())) {
					mui.toast('手机号码格式错误');
