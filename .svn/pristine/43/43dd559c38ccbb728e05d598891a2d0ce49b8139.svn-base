				$('#address1').val(province + "省" + city + "市" + district + addrarea);
				pcdaddress = province + city + district; /*获取经纬度地址*/
				//}
				searchByStationName(g,h);		
			}

			/*根据地址获取经纬度*/
			function searchByStationName(g,h) {
				var keyword = pcdaddress;
				localSearch.search(keyword);
				localSearch.setSearchCompleteCallback(function(searchResult) {
					var poi = searchResult.getPoi(0);
					if(typeof(poi) == "undefined") {
						mui.toast('地址错误');
						return;
					} else {
						$('#lng').val(poi.point.lng);
						$('#lat').val(poi.point.lat);
					}							
					var attrbul = getFormAttrs("CONSIGNEE_ADDRESS");
					$.ajax({
						type: "POST",
						url: '__URL__/SaveAgentInfo3',
						data: "attrbul=" + JSON.stringify(attrbul),
						dataType: "json",
						success: function(json) {
							var msg = eval(json);
							if(msg.code == 0) {
								settab(g, h);
							} else {
								mui.toast(msg.msg);
								return;
							}
						}
					});
				});
			}
			
			/*提交图片*/
			var resign = true;
			function shoperinfo() {
				var selectedvalue = $("#type").val();
				if(selectedvalue == 2) {
					if(!checkimg(1, 4)) {
						return false;
					}
				}else {
					if(!checkimg(1, 2)) {
						return false;
					}
				}
				if(resign) {
					resign = false;
					var attrbul = getFormAttrs('CONSIGNEE_ADDRESS');
					$.ajax({
						type: "POST",
						url: '__URL__/SaveAgentInfo5',
						data: "attrbul=" + JSON.stringify(attrbul),
						dataType: "json",
						success: function(json) {
							resign = true;
							var msg = eval(json);
							if(msg.code == 0) {
								mui.toast("保存成功！");
