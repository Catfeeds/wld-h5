			/*选项卡切换*/
			function settab(n, total) {
				for(var i = 1; i <= total; i++) {
					if(i == n) {
						$('#con_tab_' + i).css("display", "block");
						$('#tab-' + i).css("display", "block");
					} else {
						$('#con_tab_' + i).css("display", "none");
						$('#tab-' + i).css("display", "none");
					}
				}
			}
			/*上一步*/
			function PreviousCK(k, z) {
				settab(k, z);
			}
			/*下一步*/
			function NextCK(g, h) {
				if(g == 2) { /*商家类型*/
					if(checkisstore()) {
						settab(g, h);
					} else {
						mui.toast("请选择商家类型");
					}
				} else if(g == 3) { /*所属行业*/
					checktrade(g, h);
				} else if(g == 4) {
					checkiszizhi(g, h);
				} else if(g == 5) { /*gr,qy资料*/
					validateForm(g, h);
					/*省市区选中处理*/
					loadRegion('province', 2, 'city', "<?php echo U('Getbusiness/getRegion');?>");
				} else if(g == 6) { /*银行信息*/
					checkbankinfo(g, h);
				}
			}

			/*判断是否选择商家类型*/
			function checkisstore() {
				var flag = true;
				var chkRadio = $('input:radio[name="btype"]:checked').val();
				if(chkRadio == null) {
					flag = false;
				} else {
					flag = true;
					$('#isfixed').val(chkRadio);
				}
				return flag;
			}

			/*判断行业选择*/
			function checktrade(g, h) {
				var chkRadio = $('input:radio[name="trade"]:checked').val();
				if(chkRadio == null) {
					mui.toast("请选择商家类型！");
				} else {
					$('#tid').val(chkRadio);
					$.post('__URL__/SaveAgentInfo2', {
						tid: $('#tid').val()
					}, function(obj) {
						var data = eval(obj);
						if(data['code'] == 0) {
							/*线下实体只有企业资质*/
							var chkbtype = $('input:radio[name="btype"]:checked').val();
							if(chkbtype == 1) {
								checkiszizhi(g, h);
							} else {
								settab(g, h);
							}
						} else {
							mui.toast(data['msg']);
							return;
						}
					});
				}
			}

			/*判断资质选择*/
			function checkiszizhi(g, h) {
				var isfixed = $('#isfixed').val();
				var istype;
				if(isfixed == 1) {
					istype = 2;
					$('#type').val("2");
				} else {
					istype = $('input:radio[name="aptitude"]:checked').val();
					if(istype == null) {
						mui.toast("请选择商家资质！");
						return;
					} else {
						$('#type').val(istype);
					}
				}
				$.post('__URL__/SaveAgentInfo1', {
					isfixed: isfixed,
					type: $('#type').val()
				}, function(obj) {
					var data = eval(obj);
					if(data['code'] == 0) {
						if(isfixed == 1) {
							$('.qy').show();
							$('.gr').hide();
							settab(4, 6);
							styles();
						} else {
							if(istype == 1) {
								$('.gr').show();
								$('.qy').hide();
							} else if(istype == 2) {
								$('.qy').show();
								$('.gr').hide();
							}
							settab(g, h);
							styles();
						}
					} else {
						mui.toast(data['msg']);
						return;
					}
				});
			}

