		window.onload = function() {
			$('.xiaomi-bar').css('display', 'none');
			$('.mui-content').css('margin-top', '0');

			var sdh = $('.search-default').height();
			$('.search-default p').css('line-height', sdh + 'px');

			yangshi();
			$('.search-list').css('margin-top',$('.search-head').height()+'px');
		}

