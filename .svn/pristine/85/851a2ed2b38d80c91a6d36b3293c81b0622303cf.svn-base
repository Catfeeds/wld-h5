/* CSS Document */
img{width: 100%;height: 100%;}
.ccd{ color: #ffaa3d;}
.ccb{color: #56b2fb}
.cfe{ color:#f6292e;}
/*商家管理首页*/
.comm-nav-tab{padding: 0 0 0 0;-webkit-box-shadow:0 0 6px rgba(0, 0, 0, .1);-moz-box-shadow:0 0 6px rgba(0, 0, 0, .1);box-shadow:0 0 6px rgba(0, 0, 0, 0.1);margin-bottom: 1%;}
.comm-nav-tab ul{overflow: hidden;}
.comm-nav-tab ul li{float: left;padding: 3% 0;width: 50%;text-align: center;color: #999;}
.comm-nav-tab ul li.hover{border-bottom: solid 2px #47ABF7;color: #47ABF7;}

.sjcollage-con{}
.sjcollage-li{margin-bottom: 3%;}
.sjcollage-pro{padding: 3%;}
.sjc-pro-img{width: 30%;}
.sjc-pro-info{width: 67%;margin-left: 3%;}
.sjc-pro-name{}
.sjc-pro-price{}
.sjc-pro-price span{margin-right: 3%;}
.sjc-pro-price span:nth-child(2){text-decoration:line-through;}
.sjc-pro-number{}
.sjc-pro-number span{margin-right: 4%;}
.sjc-pro-number span em{font-style: normal;}
.sjc-pro-time{}
.sjcollage-oprate{ padding: 1% 3% 3% 3%;}
.oprate-time{width: 35%; padding-top: 2%;}
.oprate-time p{color:#F72A2F ;}
.oprate-time p span{margin: 0 1%;}
.oprate-state{width: 67%;overflow: hidden;}
.state-txt{}
.state-icon{width: 10%;margin-left: 2%;}
/*.oprate-btn{width: 33%;}*/
.oprate-btn{width: 65%;}
.oprate-btn span{border: solid 1px #47ABF7;color: #47ABF7;padding: 3% 7%;text-align: center;border-radius: 5px; float: right; margin-left: 4%;}
.cg{ color: #26cf8a;}


/* 参加团购css */
.collage-join{margin-top: 3%;}
.cj-order{ padding: 3%;margin-bottom: 3%;}
.cj-order-img {width: 25%;margin-right: 3%;}
.cj-order-info{width: 72%;}
.cj-order-price span:nth-child(2){text-decoration:line-through;margin-left: 5%;}
.cj-playways{ padding-left: 2%;}
.cj-pw-time{ text-align: center;line-height: 2;padding: 10% 0;}
.cj-status-img{ width: 20%;top: 12%;}
.cj-pw-djs span{margin: 0 1%;}

.visit-btn{background: #47ABF7;border-radius: 8px;padding: 1.5% 0;width: 55%;margin: 3% auto;}
.cj-pw-mebbox{ padding: 2% 0;border-top: 1px solid #ebebeb;border-bottom: 1px solid #ebebeb;margin: auto;overflow: hidden;}        
.cj-width{width:77%;margin-left:3%;}
.cj-pw-font{width: 20%;}

.cj-pw-member{ float: left; margin-right: 1%;width: 11%;}
.cj-member-img{ overflow: hidden;top: 0;left: 0;bottom: 0;right: 0;width: 100%;}
.cj-member-img img{border:solid 1px #ebebeb;}
.cj-prompt-number{width: 25%;}

.group-by-t{padding: 3% 0;border-bottom: 1px solid #ebebeb;margin: auto;overflow: hidden;}
.group-by-t span{display: block;float: left;}
.group-by-t span:nth-of-type(1){ width:20%;margin-right: 3%;}
.group-by-t span:nth-of-type(2){ width:77%;}


.cj-pw-explain{ padding: 4% 3% 4% 3%;}
.cj-explain-step{width: 75%;margin: 6% auto 2% auto;padding-left: 4%;}
.cj-exp-step div{ text-align: center;}
.cj-exp-step div:nth-of-type(1){ width:38%;}
.cj-exp-step div:nth-of-type(2){ width:31%;}
.cj-exp-step div:nth-of-type(3){ width:31%;}
.cj-hint{padding: 5% 0 20% 0;}




/*砍价详情*/
.cj-pw-time{ text-align: center;line-height: 2;padding: 10% 0;}
.cj-status-img{ width: 20%;top: 12%;}
.cj-pw-djs span{margin: 0 1%;}

.bargain-state{padding: 0 3% 6% 3%;}
.bstate-img{width: 15%;margin: 5% auto;}

.bargain-rule{padding: 3%;}
.bargain-rule li{width: 50%;}
.bargain-rule li:nth-of-type(1){text-align: left;}
.bargain-rule li:nth-of-type(2){text-align: right;}

.bargain-progress{padding: 7% 4% 4% 3%;}
.bargain-jd{background: #ebebeb;border-radius: 25px;}
.bargain-jd span{background: url(../../img/myacts/kj_plan_02.png) repeat-x;background-size: 100% 100%;}
.bargain-info{padding: 3% 0;}
.progres-float{ background: url(../../img/myacts/prgress_03.png) no-repeat;background-size: 100% 100%; top: 0;left: 0;z-index: 99;padding: 0 3% 1% 3%;}



.bargain-panel{}
.panel-tit{border-top: solid 1px #ebebeb; margin-left: 3%; overflow: hidden;padding: 4% 0;}
.panel-tit span{display: block;}
.panel-tit span:nth-of-type(1){width: 6%;margin-left: 22.5%;}
.panel-tit span:nth-of-type(2){width: 32%;margin-left: 1%;}

.panel-table-view{}
.panel-table-view li{border-bottom: solid 1px #ebebeb;margin-left: 3%;overflow: hidden;}

.red-getdetail{ padding-bottom: 5%;}
.red-getdetail-con{ padding: 3% 3% 3% 0; margin: 0 0 0 3%;overflow: hidden;}
.getdetail-img{ width: 15%;margin-right: 3%;overflow: hidden;}
.getdetail-shop{ width: 50%;}
.getdetail-money{ width: 32%;text-align: right;color: #ffaa3d;}

.bargain-b-btn{text-align: center;padding: 0;z-index: 99;}
.bargain-buy,.go-bargain{width: 50%;}
.bargain-buy{background: #FDAB3D;padding: 2% 0;}
.go-bargain{background: #FF6F33;padding: 2% 0;}

/*砍价玩法css*/
.b-explain {padding: 4%;}
.b-explain-step{width: 80%;margin: 5% auto 2% auto;}
.b-exp-step div{ width: 25%; text-align: center; margin-bottom: 5%;}
.c-box-number div{ top: 10%; left: 25%;}




/*选择型号*/
.mode-sel-bg{}
.model-sel-pup{display: none; position: fixed;left: 0;right: 0;width: 100%;margin: 0 auto;bottom: 0;z-index: 9999;max-width: 720px;}
.model-sel-main{margin-bottom: 15%;}
.m-pro-basic{padding:3% 3% 3% 0;margin-left: 3%;overflow: hidden;}
.m-pro-img{width: 20%;overflow: hidden;}
.m-pro-info{width: 77%;margin-left: 3%;}
.m-pro-info p em{margin-left: 3%;font-style: normal;}
.m-pro-info p span{}
.m-sel-close{width: 8%;right: 3%;top: 3%;}

.m-sel-list{padding: 3% 3% 3%　0;margin-left: 3%;width: 97%;}
.m-sel-list-t{padding: 3% 0 0 0;}
.m-seleted-c{padding: 2% 3% 3% 0;overflow-y: auto;-ms-overflow-y: auto;margin: 1% 0 3% 0;}
.m-seleted-c ul{overflow: hidden;width: 100%;}
.m-seleted-c ul li{background: #f0f0f0;padding: 1% 2.5%;border-radius: 5px;color: #999;float:left;margin: 0 2% 2% 0;width: 23%;text-align: center;}
.m-seleted-c ul li.hover{background: #47ABF7;color: #fff;}

.m-seleted-c span{background: #f0f0f0;padding: 1% 2.5%;border-radius: 10px;color: #999;margin: 2% 2% 2% 0;}
.m-seleted-c span.hover{background: #47ABF7;color: #fff;}

.m-pro-number{padding: 3% 3% 3%　0;margin-left: 3%;overflow: hidden;}
.m-num-opreate{padding: 3% 0;}
.m-num-sub{width: 10%;padding: 0;border-radius: 5px;}
.m-num-txt{width: 20%;overflow: hidden;margin: 0 2%;}
.m-num-txt input[type="text"]{ background: #f0f0f0; padding: 12% 0;border-radius: 5px;width: 100%;}
.m-num-add{width: 10%;padding: 0;border-radius: 5px;}

.product-stock{width: 40%;color: #555;margin-left: 3%;}
.product-stock span{color: #333;}

.m-btn-list{ position: fixed; bottom: 0;left: 0;right: 0;margin: 0 auto;max-width: 720px;}
.m-btn-list span{display: block; background: #45AAFA; width: 100%;text-align: center;padding: 2.5% 0;}


/*拼团详情*/
.pt-pro-con{border-bottom: 1px solid #eff0f5; padding: 3%; }
.pt-pro-img{ width: 25%; margin-right: 3%;}
.pt-pro-info{ width: 72%;}
.pt-pro-price span{ text-decoration: line-through; padding-left: 2%;}
.pt-pro-state{ width: 25%; right: -2%; bottom: 12%; text-align: center;}
.pt-college-img{ width: 70%;}

.pt-number-con{ padding: 6% 0;}
.pt-number-con ul{ padding: 0 18%;}
.pt-number-con ul li{ float: left; width: 18%; margin: 2% 1%; overflow: hidden; border-radius: 50%; display: block;}
.pt-number-img{ border-radius: 50%; border: 1px solid #eff0f5; overflow: hidden; width: 100%;}
.pt-number-bg,.pt-pro-bg{ width: 100%; height: 100%; background: rgba(0,0,0,0.5); bottom: 0;}
.pt-time-state{ text-align: center;}
.pt-time-state p:nth-of-type(1){ padding: 2.5% 0;}
.pt-time-state .pt-time-show p {color: #F72A2F; font-weight: bold; padding: 0;}
.pt-time-show p span{ margin: 0 1%;}
.pt-time-show p font{ color:#333;}

.pt-title{ font-weight: bold; padding-bottom: 2%;}
.pt-title font{display: block; width: 5px; height: 1.5rem; background: #000;float: left;margin: 0.5% 2% 0 0;}

.pt-playwy-con{ border-top: 8px solid #f0f0f0; border-bottom: 8px solid #f0f0f0; padding: 5% 3%;}
.pt-playwy-con p{ margin: 2% auto;}
.pt-playwy-con p font{ display: block; width: 4px; height: 4px; border-radius: 50%; background: #000; float: left; margin: 2% 1.5% 0 0;}
.pt-help-img{ width: 6%; right: 1%; top: 0; z-index: 99;}

.pt-good-remark{ padding: 3%; margin-top: 2.5%;}
.pt-good-imglist{ max-width: 720px;}

.pt-buy-btn{ overflow: hidden; position: fixed; text-align: center;width: 100%; max-width: 720px; min-width: 320px; margin: auto; left: 0; right: 0; bottom: 0; z-index: 999;}
.server-btn,.shop-btn { width: 15%; display: block; background: #fff; padding: 1% 0; }
.server-btnimg,.shop-btnimg{ width: 55%; margin: 0 auto;}
.pt-shopping-text{ width: 70%; background: #ff4a49; padding: 1.1% 0;}
.pt-shopping-text span{ display: block;}