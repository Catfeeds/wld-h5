<body>
     <!-- 导航部分 -->
    <nav class="breadcrumb">
        <i class="Hui-iconfont">&#xe67f;</i> 收银员管理
        <span class="c-gray en">&gt;</span> 收银员上班记录
        <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a>
    </nav>
<div class="pd-20 text-c">
    <!-- 搜索部分 -->
	<form action="" method="get">
	<div class="text-c">
	    条件搜索
		<input type="text" class="input-text" style="width:150px" placeholder="日期" name="c_datetime" value="{$post['c_datetime']}">
		<input type="hidden" name="ucode"  value="{$ucode}">
		<button type="submit" class="btn btn-success radius" id="" name=""><i class="Hui-iconfont">&#xe665;</i> 搜索</button>
	</div>
	</form>
	<!-- 数据列表 -->
	<div class="mt-20">
		<table class="table table-border table-bordered table-bg table-hover table-sort">
			<thead>
				<tr class="text-c">
					<th width="25"><input type="checkbox" name="" value=""></th>
					<!-- <th width="80">收银员昵称</th>
					<th width="80">收银员账号</th>
					<th width="80">所属商家昵称</th> -->
					<th width="80">收银台编号</th>
					<th width="80">收银台名称</th>
					<th width="120">日期</th>	
					<th width="120">签到时间</th>					
					<th width="120">下班时间</th>
				</tr>
			</thead>
			<tbody>
				<volist name="list" id="vo">
				    <tr class="text-c">
					    <td><input type="checkbox" name="bankid" value="{$vo.c_id}"></td>
						<!-- <td>
							<a title="用户详情" href="__HHOME__/Member/member_list?ucode={$vo.c_ucode}" style="text-decoration:none">{$vo['c_nickname']}
							</a>
						</td>
						<td>{$vo['c_phone']}</td>
			            <td>
			            	<a title="商家详情" href="__HHOME__/Member/member_list?ucode={$vo.c_acode}" style="text-decoration:none">{$vo['anickname']}
							</a>
						</td> -->
						<td>{$vo['c_deskid']}</td>
						<td>{$vo['c_names']}</td>
						<td>{$vo['c_datetime']}</td>
						<td>{$vo['c_signtime']}</td>
						<td>{$vo['c_leavetime']}</td>
					</tr>
				</volist>				
			</tbody>
		</table>
	</div>
</div>

<!-- 分页 -->
<div class="pages">{$page}</div>		
