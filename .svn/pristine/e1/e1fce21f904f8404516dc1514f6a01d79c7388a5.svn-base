.goods_name{width: 50px;overflow: hidden;padding:1% 0px 1% 1%;}
.goods_con{width: 70%;overflow: hidden;border:0px;padding:1.5% 0px;}

.productmodular-bottom{width: 80%;overflow: hidden}
.productmodular-img{position: relative;float:left;width: 15%;margin-right: 2%;margin-top:3%;}
.productmodular-position{position: absolute;width: 25%;top:35%;left:35%;overflow: hidden;
	}
.productmodular-img img,.productmodular-position img,.productmodular-position img
