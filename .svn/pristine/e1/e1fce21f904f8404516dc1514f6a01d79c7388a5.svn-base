.model_span{width: 15%;background:#ebebeb;text-align:center;border:1px solid #ebebeb;
border-radius: 3px;}
.model_write{width: 15%;padding:1% 0px;overflow: hidden;border:1px solid #ebebeb;
border-radius: 3px;text-align:center;}
.model_del img{width: 10%;overflow: hidden;margin:2% 45%;}
.model_add{width: 95%;overflow: hidden;text-align: center;color:#333;border:1px dashed #ebebeb;
