@charset "utf-8";
/* CSS Document */
img{width: 100%;height: 100%;}

/****************************/

.server-list{width: 100%;overflow: hidden;}
.server-list ul li{ background: #fff;border-radius: 5px; float: left;width: 45%; margin: 2.5% 0 0 3.5%; padding: 8.5% 4%;}
.server-list ul li img{width: 100%;height: 100%;}
.server-list ul li a{display: block;}
.server-li-img{width: 50%;overflow: hidden;margin-left: 25%;}
.server-li-txt{text-align: center;padding-top: 3%;color: #555;font-size: 14px;}
.server-list ul li.li-r-none{border-right: none;}
.server-list ul li.li-b-none{border-bottom: none;}

.server-marketing{border-top: 1px #f0f0f0 solid; width: 100%;padding: 4%;}
.server-marketing a{display: block;width: 80%;margin: 0 auto;text-align: center;position: relative;}
.marketing-urltit{display: block;position: absolute;text-align: center;width: 100%;}



