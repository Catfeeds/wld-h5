			//用户签到
			function Usersign() {
				var province = $('#province').val();
				var city = $('#city').val();
				var provcode = $('#provincecode').val();
				var ccode = $('#citycode').val();
				if(!provcode || !ccode) {
					mui.alert('请稍后再试');
					return;
				}
				var urls = '__URL__/Usersign?province=' + province + '&city=' + city + '&provincecode=' + provcode + '&citycode=' + ccode;

				$.getJSON(urls, function(json) {
					var obj = eval(json);
					if(obj['code'] == 0) {
						var data = obj.data;
						var moodsnum = parseInt($('#moods').text()) + data['add_moods'];
						$('#moods').text(moodsnum);
						$('#checkin').text('已签到');
						$('#checkin').addClass('cy');
						$('#checkin').removeClass('c5');
					} else {
						mui.alert(obj.msg);
					}
				});
			}
			//获取用户签到信息
			function Signinfo() {
				var province = $('#province').val();
				var city = $('#city').val();
				var provcode = $('#provincecode').val();
				var ccode = $('#citycode').val();
				var urls = '__URL__/Signinfo?province=' + province + '&city=' + city + '&provincecode=' + provcode + '&citycode=' + ccode;

				$.getJSON(urls, function(json) {
					var obj = eval(json);
					if(obj['code'] == 0) {
						var data = obj.data;
						if(data.signstate == 1) {
							$('#checkin').text(data.msg);
							$('#checkin').addClass('cy');
							$('#checkin').removeClass('c5');
						}
					}
				});
			}
