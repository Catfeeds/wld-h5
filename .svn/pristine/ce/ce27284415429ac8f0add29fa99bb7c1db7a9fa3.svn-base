
/*系统自定义弹出框样式*/
#jd_shadow{z-index:99999;position:fixed;background-color:#333;filter:alpha(opacity=20);width: 100%;
	height: 100%;-moz-opacity:0.20;opacity:0.20; left:0; top:0;bottom: 0px;}
#jd_dialog{z-index:100000;position:absolute;overflow: hidden; left:50%; top:50%;width: 250px;height: 150px;
margin: -75px -125px;background: #ffffff;border-radius: 10px;-moz-border-radius:10px;
-webkit-border-radius:10px;}
#jd_dialog_h{width: 100%;height: 30px;line-height: 30px; overflow: hidden;text-align: center;color: #333333;font-size: 16px;}
#jd_dialog_c{width: 100%;height: 80px;overflow: hidden;font-size: 14px;padding: 0px 10px;color: #555555;}
#jd_dialog_b{width: 100%;height: 40px;font-size: 16px;color: #999999;border-top: 1px solid #ebebeb;}
#jd_dialog_l{float:left;width: 50%;height: 40px;line-height: 40px;text-align: center;margin-left: -1px;
	border-right: 1px solid #ebebeb;}
#jd_dialog_r{float:left;width: 50%;height: 40px;line-height: 40px;text-align: center;}
#jd_dialog_l:hover,#jd_dialog_r:hover,#jd_dialog_l:active,#jd_dialog_r:active{color: #49abf7;}


#jd_show_time{z-index:100001;position:fixed;width: 150px;height: 80px;overflow: hidden;
	padding: 5px;top: 30%;left: 50%;margin: -40px -75px;color: #ffffff;
	background-color:rgba(51,51,51,0.8);border-radius: 10px;-moz-border-radius:10px;
-webkit-border-radius:10px;font-size: 14px;}