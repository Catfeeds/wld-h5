			/*数据加载*/
			function getdatalist(statu) {
				var url = "?pageindex=" + pageindex + "&type=" + statu;
				var _html = "";
				$.ajax({
					type: 'get',
					dataType: 'json',
					url: url,
					cache: false,
					beforeSend: function() {
						$('#console_' + statu).css('display', 'block');
						$('#console_' + statu).html('加载中...');
						ctrls = false;
					},
					error: function() {
						$('#console_' + statu).css('display', 'block');
						$('#console_' + statu).html('加载失败');
						ctrls = true;
					},
					success: function(obj) {
						if(pageindex == 1) {
							$('#data_list_' + statu).empty();
						}
						var mgs = eval(obj);
						if(mgs['code'] == 0) {
							var data = mgs.data;
							if(data == null || data.list == null) {
								if(pageindex == 1) {

								}
								emptyval = false;
							} else {
								if(pageindex <= data.pageCount) {
									pageindex++;
									var datalist = data.list;
									for(var i = 0; i < datalist.length; i++) {

									};
								} else {
									emptyval = false;
								}
							}
						} else {
							emptyval = false;
						}
						$('#data_list_' + statu).append(_html);
					},
					complete: function() {
						styles();
						$('#console_' + statu).css('display', 'none');
						ctrls = true;
					}
				});
			}

			function newindex() {
				if(tjsign) {
					tjsign = false;
					var re = /^[0-9]*[1-9][0-9]*$/;
					var codenum = $('#code_num').val();
					if(!codenum) {
						alert('请输入申请串码个数');
						tjsign = true;
						return false;
					}　　
					if(!re.test(codenum)) {　　　　
						alert("请输入数字");　　　　
						$('#code_num').val("");
						tjsign = true;　　　　
						return false;　　
					}

					$.post("__URL__/ApplyCode", {
						num: codenum
					}, function(obj) {
						var data = eval(obj);
						if(data['code'] == 0) {
							alert(data['msg']);
							window.location.href = "__URL__/index";
						} else {
							alert(data['msg']);
							tjsign = true;
						}
					});
				}
			}


