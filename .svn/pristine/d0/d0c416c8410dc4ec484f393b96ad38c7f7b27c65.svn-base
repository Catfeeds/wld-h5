@charset "utf-8";
/* CSS Document */
img{width: 100%;height: 100%;}

/* 首页 */
.mshop-head{background: #46a9fa; text-align: center;padding: 4% 0 8% 0;}
.mshop-head-img{ width: 18%; margin: 0 auto 2% auto;}
.mshop-head p:nth-of-type(2){ color: #a2d3fb;}

.mshop-money{ border-bottom: 1px solid #f6f6f6; padding: 6% 0;}
.mshop-money ul{ width: 100%; overflow: hidden;}
.mshop-money ul li{ width: 50%; float: left; text-align: center;}
.mshop-money ul li p{ padding-bottom: 5%;}
.mshop-report-tab{ text-align: center;padding: 2% 3% 0 0; border-bottom: 1px solid #cdcdcd;}
.mshop-report-tab ul{ padding-left: 18%;}
.mshop-report-tab ul li{ float: left; width: 40%; padding-bottom:5%;}
.mshop-report-tab ul li.report-bg{ border-bottom: 2px solid #46a9fa; color: #46a9fa; font-weight: bold;}
.mshop-report-time{ width: 11%;}
.mshop-menu{ border-top: 1px solid #f6f6f6; padding: 3.5% 2% 3% 4%;}
.mshop-menu-next{ width: 6%;}


/*跨界收益*/
.mshop-cin-list ul{ padding-left: 3%;}
.mshop-cin-list ul li{ width: 100%; overflow: hidden; border-bottom: 1px solid #f6f6f6; padding: 3% 3% 3% 0;}
.mshop-cin-img{ width: 16%; margin-right: 3%; overflow: hidden; border: 1px solid #f0f0f0;}
.mshop-cin-info{ width: 50%;}


/*营收明细*/
.incdt-total-money{ border-bottom: 1px solid #f6f6f6;text-align: center; padding: 5% 0;}
.incdt-total-money p{ padding-bottom: 2%;}
.mshop-item-tit{ padding: 3.5% 3% 3.5% 0;border-bottom: 1px solid #f6f6f6; margin-left: 3%;width: 97%;}