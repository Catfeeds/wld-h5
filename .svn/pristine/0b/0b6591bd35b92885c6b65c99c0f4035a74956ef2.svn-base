		<script type="text/javascript">
			var inputreg = /^[\s]*$/;
			var bbname = '<?php echo $info["c_bankname"] ?>';
			var subbname = '<?php echo $info["c_sub_bankname"] ?>';
			window.onload = function() {
				var btnh = $('.bind-info-btn').height(),
					fonth = btnh * 78 / 100;
				$('.bind-info-btn').height(btnh);
				$('.btnfont').css("line-height", fonth + "px");

				$('.resource-l').css('line-height', $('.resource-list').height() + 'px');

				var ynh = $('.yourname-text').height();
				$('.yourname').css('line-height', ynh + 'px');
				$('.yourname-text').css('line-height', ynh + 'px');

				$('#return-page1').click(function() {
					$('#con_tab_5').css("display", "block");
					$('#bk_name_pup').css("display", "none");
					$('#ss_name_pup').css("display", "none");
				});
				$('#return-page2').click(function() {
					$('#con_tab_5').css("display", "block");
					$('#bk_name_pup').css("display", "none");
					$('#ss_name_pup').css("display", "none");
				});
			}

			function formatBankNo(BankNo) {
				if(BankNo.value == "") return;
				var account = new String(BankNo.value);
				account = account.substring(0, 25);
				/*帐号的总数, 包括空格在内 */
				if(account.match(".[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{7}") == null) {
					/* 对照格式 */
					if(account.match(".[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{7}|" + ".[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{7}|" +
							".[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{7}|" + ".[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{7}") == null) {
						var accountNumeric = accountChar = "",
							i;
						for(i = 0; i < account.length; i++) {
							accountChar = account.substr(i, 1);
							if(!isNaN(accountChar) && (accountChar != " ")) accountNumeric = accountNumeric + accountChar;
						}
						account = "";
						for(i = 0; i < accountNumeric.length; i++) { /* 可将以下空格改为-,效果也不错 */
							if(i == 4) account = account + " ";
							/* 帐号第四位数后加空格 */
							if(i == 8) account = account + " ";
							/* 帐号第八位数后加空格 */
							if(i == 12) account = account + " ";
							/* 帐号第十二位后数后加空格 */
							account = account + accountNumeric.substr(i, 1)
						}
					}
				} else {
					account = " " + account.substring(1, 5) + " " + account.substring(6, 10) + " " + account.substring(14, 18) + "-" + account.substring(18, 25);
				}
				if(account != BankNo.value) BankNo.value = account;
			}

			$

			$(function() {
				/*判断是否绑定银行卡*/
				if(bbname && subbname) {
					mui(".mui-content").off("tap", "#bindbtn_1", update_user_bank);
					mui('.mui-content').on('tap', "#bindbtn_1", function() {
						mui.confirm('解除绑定后银行卡提现服务将不可使用', '确认解除绑定？', ['取消', '确认'], function(e) {
							e.index == 0 ? "" : unbindcard();
						}, 'div');
					});
				} else {
					mui(".mui-content").off("tap", "#bindbtn_1", unbindcard);
					mui('.mui-content').on('tap', "#bindbtn_1", function() {
						update_user_bank();
					});
				}
			});
			var bindtag = true;
			/*绑定*/
			function update_user_bank() {
				if(bindtag) {
					bindtag = false;
					var reg = /^[\u4e00-\u9fa5 ]{2,20}$/;
					if(inputreg.test($('#bankname').val())) {
						mui.toast('请选择开户行');
						$('#bankname').focus();
						bindtag = true;
						return false;
					}
					if(inputreg.test($('#branchname').val())) {
						mui.toast('请选择支行');
						$('#branchname').focus();
						bindtag = true;
						return false;
					}
					if(!$('input[name=banksn]').val()) {
						mui.toast("请输入银行卡号");
						bindtag = true;
						return false;
					}

					var province = $("#province option:selected").text();
					var city = $("#city option:selected").text();
					if(province == "选择省" || city == "请选择") {
						mui.toast("请完善银行资料信息！");
						$("#province").focus();
						bindtag = true;
						return false;
					} else if(province == "选择省" || city == "选择市") {
						mui.toast("请完善银行资料信息！");
						$("#province").focus();
						bindtag = true;
						return false;
					}

					var attrbul = getFormAttrs('CONSIGNEE_ADDRESS');
					$.ajax({
						type: "POST",
						url: '__URL__/bindingbank',
						data: "attrbul=" + JSON.stringify(attrbul),
						dataType: "json",
						success: function(json) {
							var msg = eval(json);
							if(msg.code == 0) {
								mui.alert(msg.msg, '提示', '确定', function(e) {
									$('#bindbtn_1').text("解除解绑");
									$("#searchbank").prop("onclick", null).off("click");
									$('#searchson').prop("onclick", null).off("click");
									$("#province").attr("disabled", "disabled");
									$("#city").attr("disabled", "disabled");
									$("#banksn").attr("disabled", "disabled");

									mui(".mui-content").off("tap", "#bindbtn_1", update_user_bank);
									mui('.mui-content').on('tap', "#bindbtn_1", function() {
										mui.confirm('解除绑定后银行卡提现服务将不可使用', '确认解除绑定？', ['取消', '确认'], function(e) {
											e.index == 0 ? "" : unbindcard();
										}, 'div');
									});
								}, 'div');
							} else {
								mui.toast(msg.msg);
								bindtag = true;
							}
						}
					});

				}
			}
			/*解绑*/
			var flagc = true;

			function unbindcard() {
				if(flagc) {
					flagc = false;
					$.post("__URL__/RelieveBank", function(obj) {
						var data = eval(obj);
						if(data['code'] == 0) {
							mui.toast('解绑成功！');
							$('#bindbtn_1').text("立即绑定");

                            $("#searchbank").attr("onclick", "searchbank()");
							$("#searchson").attr("onclick", "searchson()");
							$("#province").removeAttr("disabled");
							$("#city").removeAttr("disabled");
							$("#banksn").removeAttr("disabled");

							mui(".mui-content").off("tap", "#bindbtn_1", unbindcard);
							mui('.mui-content').on('tap', "#bindbtn_1", function() {
								update_user_bank();
							});
							setTimeout(function() {
								location.reload();
							}, 1000);

						} else {
							mui.toast(data['msg']);
							flagc = true;
						}
					});
				}
			}

			/*开户行搜索弹窗*/
			function searchbank() {
				$('#con_tab_5').css("display", "none");
				$('#ss_name_pup').css("display", "none");
				$('#bk_name_pup').css("display", "block");
				getdatalist();
			}
			/*支行弹窗*/
			function searchson() {
				var bankname = $('#bankname').val();
				if(!bankname) {
					alert('请先选择开户银行');
					return;
				}
				$('#con_tab_5').css("display", "none");
				$('#bk_name_pup').css("display", "none");
				$('#ss_name_pup').css("display", "block");
				getbankname();
			}

			/*数据加载*/
			function getdatalist() {
				$('#data_list').empty();
				var name = $('#bankkey').val();
				var url = "__APP__/Home/Getbusiness/GetBankList?name=" + name;
				var _html = "";
				$.ajax({
					type: 'get',
					dataType: 'json',
					url: url,
					cache: false,
					beforeSend: function() {
						$('#console').css('display', 'block');
						$('#console').html('加载中...');
					},
					error: function() {
						$('#console').css('display', 'block');
						$('#console').html('加载失败');
					},
					success: function(obj) {
						var mgs = eval(obj);
						if(mgs['code'] == 0) {
							var data = mgs.data;
							if(data) {
								for(var i = 0; i < data.length; i++) {
									var dataarr = data[i];
									_html += '<li onclick="sel_fee_bank(\'' + dataarr['c_name'] + '\')">';
									_html += '<div class="bankcell">';
									_html += '' + dataarr['c_id'] + '';
									_html += '</div>';
									_html += '<div class="bankcell">';
									_html += '' + dataarr['c_name'] + '';
									_html += '</div>';
									_html += '</li>';
								};
							} else {
								_html += '<div class="baoqian">没有找到相关信息</div>';
							}
						} else {
							_html += '<div class="baoqian">没有相关信息</div>';
						}
						$('#data_list').append(_html);
					},
					complete: function() {
						$('#console').css('display', 'none');
					}
				});
			}

			/*支行列表*/
			function getbankname() {
				$('#data_list_2').empty();
				var bankname = $('#bankname').val();
				var name = $('#bank_zhkey').val();
				var url = "__APP__/Home/Getbusiness/GetBranchList?bankname=" + bankname + "&name=" + name;
				var _html = "";
				$.ajax({
					type: 'get',
					dataType: 'json',
					url: url,
					cache: false,
					beforeSend: function() {
						$('#console').css('display', 'block');
						$('#console').html('加载中...');
						kongzhi = false;
					},
					error: function() {
						$('#console').css('display', 'block');
						$('#console').html('加载失败');
						kongzhi = false;
					},
					success: function(obj) {
						var mgs = eval(obj);
						if(mgs['code'] == 0) {
							var data = mgs.data;
							if(data) {
								for(var i = 0; i < data.length; i++) {
									var dataarr = data[i];
									_html += '<li onclick="sel_bank_name(\'' + dataarr['c_name'] + '\',\'' + dataarr['c_code'] + '\')">';
									_html += '<div class="bankcell">';
									_html += '' + dataarr['c_id'] + '';
									_html += '</div>';
									_html += '<div class="bankcell">';
									_html += '' + dataarr['c_name'] + '';
									_html += '</div>';
									_html += '</li>';
								};
							} else {
								_html += '<div class="baoqian">没有找到相关信息</div>';
							}
						} else {
							_html += '<div class="baoqian">没有相关信息</div>';
						}
						$('#data_list_2').append(_html);
					},
					complete: function() {
						$('#console').css('display', 'none');
						kongzhi = true;
					}
				});
			}

			/*选择开户行*/
			function sel_fee_bank(name) {
				$('#bankname').val(name);
				$('#fee_banktext').text(name);
				$('#con_tab_5').css("display", "block");
				$('#bk_name_pup').css("display", "none");
				$('#ss_name_pup').css("display", "none");
			}

			/*请选择支行*/
			function sel_bank_name(name, actline) {
				$('#branchname').val(name);
				$('#banknametext').text(name);
				$('#contactline').val(actline);
				$('#con_tab_5').css("display", "block");
				$('#bk_name_pup').css("display", "none");
				$('#ss_name_pup').css("display", "none");
			}
		</script>

		<include file="Common/footer" />

	</body>

</html>