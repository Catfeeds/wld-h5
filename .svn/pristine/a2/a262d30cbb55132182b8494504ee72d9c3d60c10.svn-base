@charset "utf-8";
/* CSS Document */
img{width: 100%;height: 100%;}


/*  首页  */
.js-tab{ margin-bottom: 1%;}
.js-tab ul li{ width: 50%; float: left; text-align: center; padding: 3.5% 0;}
.js-tab ul li.hover{ border-bottom: 2px solid #45aafa;}
.js-total-money{ padding: 4% 0 6% 3%;}
.js-total-money span{ padding-left: 3%;}

.js-report-data{ padding-bottom: 7%;}
.store-report-income{ width: 94%; margin: 0 auto; padding: 4% 0; border: 1px solid #c9c9c9;}
.store-report-income ul li{ position: relative; width: 33.3%; float: left; text-align: center;}
.store-income-box p{ padding-bottom: 4%;}
.rline{ display: block;height: 1px;width: 33%;background: #c9c9c9;transform: rotate(90deg);top: 45%;position: absolute;right: -17%;}

.store-bill-con{ padding: 0 0 0 3%; border-top: 1px solid #ebebeb;}
.store-bill-time{ padding: 3% 3% 3% 0;}
.bill-time-img{ width: 9%;}
.store-bill-item{ border-bottom: 1px solid #ebebeb; padding: 1% 0 4% 0; margin-bottom: 3%;}
.store-bill-item:last-child { margin-bottom:0; }
.store-bill-img{ width: 15%; overflow: hidden; margin-right: 3%;}
.store-bill-info{ width: 35%;}
.store-bill-info{ width: 35%;}
.store-bill-info div{ padding-top:2%;}
.store-bill-item p{ width: 22.5%;}



/* 我的加盟店  */
.js-myinfo{ width: 85%; margin: 16% auto;}
.js-myinfo-con{ top:15%;}
.mystore-img{ width: 22%; margin: 0 auto; overflow: hidden;}
.mystore-name{ text-align: center; padding: 5% 0;}
.js-myinfo-con p{ padding: 0 0 3% 8%;}